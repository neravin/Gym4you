<!DOCTYPE html>
<html>
<head>
  <title>GeoFitness</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <link rel="stylesheet" href="//brick.a.ssl.fastly.net/Alegreya:400,700,400i,700i">
  <!--%= javascript_include_tag "http://maps.googleapis.com/maps/api/js?sensor=false&callback=initialize", "data-turbolinks-track" => true %--> 
  <%= csrf_meta_tags %>
</head>
<body class="<%= controller.controller_name %>">
<%= render 'layouts/header' %>
<section id = "grid-main">
	<div id="grid-wrap-menu" class="grid-wrap clearfix">
		<%= yield %>
	</div><!--/grid-main-->
</section><!--/grid-wrap-->
<%= render 'layouts/footer' %>
<%= javascript_include_tag "http://maps.googleapis.com/maps/api/js?sensor=false&callback=initialize", "data-turbolinks-track" => true %> 
</body>
</html>
